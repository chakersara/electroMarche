{% block header %}


    <header class="header shop">
        <!-- Topbar -->
{% include "header.html.twig" %}
        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="cat-nav-head">
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="all-category">
                                <h3 class="cat-heading"><i class="fa fa-bars" aria-hidden="true"></i>CATEGORIES</h3>
                                <ul class="main-category">
                                    {% for pcMap,cMap in hehi %}
                                    <li><a href="/products/{{ pcMap|replace({' ':'-'})}}">{{ pcMap}}
                                            {% if cMap|length>0 %}
                                            <i class="fa fa-angle-right" aria-hidden="true"></i>
                                            {% endif %}
                                        </a>

                                        {% if cMap|length>0 %}
                                            <ul class="sub-category row p-2" style="width: 1100px">

                                            {% for elem in cMap %}
                                                <div class="col-4" ><li ><a class="align-middle" href="/products/{{ pcMap|replace({' ':"-"}) }}/{{ elem.ctg|replace({' ':"-"}) }}">{{ elem.ctg }}</a></li></div>

                                            {% endfor %}
                                            </ul>

                                        {% endif %}
                                    </li>
                                    {% endfor %}


                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-9 col-12">
                            <div class="menu-area">
                                <!-- Main Menu -->
                                <nav class="navbar navbar-expand-lg">
                                    <div class="navbar-collapse">
                                        <div class="nav-inner">
                                            <ul class="nav main-menu menu navbar-nav">
                                                <li class="active"><a href="/">Home</a></li>
                                                <li><a href="/products">Product</a></li>
                                                <li><a href="#">Shop<i class="ti-angle-down"></i><span class="new">New</span></a>
                                                    <ul class="dropdown">
                                                        <li><a href="shop-grid.html">Shop Grid</a></li>
                                                        <li><a href="cart.html">Cart</a></li>
                                                        <li><a href="checkout.html">Checkout</a></li>
                                                    </ul>
                                                </li>
                                                <li><a href="contact.html">Contact Us</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </nav>
                                <!--/ End Main Menu -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
