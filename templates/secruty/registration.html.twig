	<style>
		.wrap-input100 ul li{
			font-size: 12px;
			color: #B0413E;
		}
	</style>
</head>
<body>
{% block  form %}

<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<div class="login100-form-title" style="background-image: url({{ asset("security/images/bg-01.jpg") }});">
					<span class="login100-form-title-1">
						{% block name %}
							Register
						{% endblock %}
					</span>
				</div>
				{{ form_start(form) }}
					{{ form_row(form.user_name,{"label":"Username","attr":{"placeholder":"Enter username"}})}}
				{{ form_row(form.email,{"attr":{"placeholder":"Enter email"}})}}
				{{ form_row(form.password,{"attr":{"placeholder":"Enter password"}})}}
				{{ form_row(form.confirm_password,{"attr":{"placeholder":"Re-Enter email"}})}}
					<span class="flex-sb-m w-full p-b-30">

						<div class="txt1  ">
							<a href="/login" class="txt1 ">
								Already have an account?<b class="text-success"> Log in</b>
							</a>
						</div>
					</span>
				<div  class="container-login100-form-btn">
					<button type="submit" class="login100-form-btn">
						Sign in
					</button>
				</div>

				{{ form_end(form) }}

				<script defer>
					var form=document.querySelector("form")
					form.setAttribute("class","login100-form validate-form")

					var inputs=form.querySelectorAll("div")
					inputs.forEach(element=>{
						var butt=element.querySelector("button")
						var a=element.querySelector("a")
						if (butt==null && a==null){
						element.classList.add("wrap-input100","validate-input","m-b-26")
						element.querySelector('input').classList.add("input100")
						element.querySelector('label').classList.add("label-input100","text-left")}
					})
				</script>

			</div>
		</div>
	</div>
{% endblock %}

